{
  "name": "name",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "poi --serve",
    "build": "poi --prod",
    "typecheck": "tsc --noEmit",
    "sizecheck": "poi --prod --source-map --out-dir .poi && source-map-explorer .poi/assets/js/*.chunk.js --html source-map-explorer.html",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "preact": "^10.0.0-beta.2"
  },
  "devDependencies": {
    "@types/node": "^12.0.4",
    "@types/webpack-env": "^1.13.9",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "patch-package": "^6.1.2",
    "poi": "^12.6.10",
    "postcss-preset-env": "^6.6.0",
    "prettier": "^1.17.1",
    "source-map-explorer": "^2.0.0",
    "typescript": ">=3.4.5 <3.5.0"
  },
  "browserslist": [
    "> 0.5% in JP, Firefox ESR, not dead"
  ]
}
